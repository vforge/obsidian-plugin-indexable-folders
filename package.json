{
	"name": "obsidian-plugin-indexable-folders",
	"version": "0.0.4",
	"description": "An Obsidian plugin that enhances folder organization by styling numeric prefixes and providing context menu actions for reordering indexed folders.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "prettier --write ./src && eslint ./src/ --ext .ts --quiet --fix",
		"prepare": "husky"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"folders",
		"organization",
		"indexing",
		"file-explorer",
		"productivity"
	],
	"author": "vforge",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/vforge/obsidian-plugin-indexable-folders.git"
	},
	"homepage": "https://github.com/vforge/obsidian-plugin-indexable-folders#readme",
	"bugs": {
		"url": "https://github.com/vforge/obsidian-plugin-indexable-folders/issues"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.3.2",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7"
	},
	"dependencies": {},
	"lint-staged": {
		"{src,tests}/**/*.ts": [
			"prettier --write",
			"eslint --quiet --fix"
		]
	}
}
